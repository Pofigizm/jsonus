{
  "name": "jsonus",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint *.js **/*.js",
    "start": "DEBUG=jsonus:* node index.js"
  },
  "dependencies": {
    "babel-core": "6.18.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.18.0",
    "body-parser": "1.15.2",
    "debug": "2.3.2",
    "draft-js": "https://github.com/Pofigizm/draft-js.git#a442f24d573931dce10c3b043dee18e4fa86b810",
    "draft-js-export-html": "0.5.1",
    "esutils": "^2.0.2",
    "express": "4.14.0",
    "jsdom": "9.8.3",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "eslint": "3.10.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "global-require": 0,
      "no-console": 0,
      "react/jsx-filename-extension": 0,
      "semi": [
        2,
        "never"
      ],
      "angular/document-service": 0,
      "angular/window-service": 0,
      "angular/typecheck-object": 0,
      "angular/typecheck-string": 0,
      "angular/log": 0
    }
  }
}
